/*=============================================================================== .nav =====*/
.nav {
	position: relative;
}
	.nav ul {
		width:100%;
	}
	.nav li {
		float: left;
		font-size: 16px;
		line-height: 1.6em;
		background-image: url(../images/nav-li-bg.png); /** sprite-ref: style; sprite-alignment: no-repeat */
		background-repeat:no-repeat;
		padding-left: 2px; 
		/*width: 188px;*/
		width: 128px;
	}
		.nav li a {
			display:block;
			text-decoration:none;
			color: #1b1b1b;
		}
			.nav a strong {
				display: block;
				background-image: url(../images/nav-a-bg.png); /** sprite-ref: style2; sprite-alignment: repeat-x */
				background-repeat:repeat-x;
				text-align: center;
				padding: 6px 0 16px;
                height: 15px;
				max-height:15px;
			}
				.nav a:hover strong, .nav .active strong {
					background-image: url(../images/nav-a-bg-hover.png); /** sprite-ref: style2; sprite-alignment: repeat-x */
					background-repeat:repeat-x;
				}
	.nav .first {background: none;
				 padding-left: 0px;}
		.nav .first a {
			padding: 0px 0px 0px 6px;
			background-image: url(../images/nav-first-strong-bg.png); /** sprite-ref: style; sprite-alignment: no-repeat */
			background-repeat:no-repeat;
		}
			.nav .first a:hover, .nav .first .active {
				background-image: url(../images/nav-first-strong-bg-hover.png); /** sprite-ref: style; sprite-alignment: no-repeat */
				background-repeat:no-repeat;
			}
	.nav .last {padding-left: 1px;}		
	.nav .last a {
		padding: 0px 5px 0px 0px;
		background-image: url(../images/nav-last-strong-bg.png); /** sprite-ref: style; sprite-alignment: no-repeat; sprite-alignment: right; */
		background-repeat:no-repeat;
		width: 134px;
	}
		.nav .last a:hover, .nav .last .active {
			background-image: url(../images/nav-last-strong-bg-hover.png); /** sprite-ref: style; sprite-alignment: no-repeat right; sprite-alignment: no-repeat; sprite-alignment: right; */
			background-repeat:no-repeat;
		}
